  "version": "0.2.0",
  "configurations": [
    {
      "name": "Force Learning",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": ["--force"],
      "console": "integratedTerminal"
    },
    {
      "name": "Load Learning",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Save Dataset",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": ["--save"],
      "console": "integratedTerminal"
    }
  ]
}
